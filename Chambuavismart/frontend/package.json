{
  "name": "chambua-vismart-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "node --no-deprecation ./node_modules/@angular/cli/bin/ng",
    "start": "node --no-deprecation ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json",
    "serve:open": "node --no-deprecation ./node_modules/@angular/cli/bin/ng serve -o",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "check:dist": "node ./scripts/check-dist.js",
    "test": "ng test",
    "lint": "ng lint",
    "postinstall": "patch-package"
  },
  "engines": {
    "node": ">=18.13.0 <19 || >=20.9.0 <21",
    "npm": ">=9 <11"
  },
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@angular/animations": "17.3.8",
    "@angular/common": "17.3.8",
    "@angular/compiler": "17.3.8",
    "@angular/core": "17.3.8",
    "@angular/forms": "17.3.8",
    "@angular/platform-browser": "17.3.8",
    "@angular/platform-browser-dynamic": "17.3.8",
    "@angular/router": "17.3.8",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "17.3.8",
    "@angular/compiler-cli": "17.3.8",
    "@types/jasmine": "5.1.5",
    "jasmine-core": "5.1.2",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "typescript": "5.4.5",
    "patch-package": "8.0.0"
  }
}
